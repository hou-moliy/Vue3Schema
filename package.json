{
  "name": "vue3-cli-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rimraf dist && npm run build:core && npm run build:theme",
    "build:core": "TYPE=lib vue-cli-service build --target  lib --name index --no-clean lib/index.ts",
    "build:theme": "TYPE=lib vue-cli-service build --target lib --name theme-default/index --no-clean lib/theme-default/index.tsx",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:cov": "vue-cli-service test:unit --coverage",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-i18n": "^4.2.0",
    "core-js": "^3.8.3",
    "json-schema-merge-allof": "^0.8.1",
    "jsonpointer": "^5.0.1",
    "jss": "^10.10.0",
    "jss-preset-default": "^10.10.0",
    "lodash.topath": "^4.5.2",
    "lodash.union": "^4.6.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "reset-css": "^5.0.2",
    "vue": "^3.2.13",
    "vue-class-component": "^8.0.0-0",
    "vue-jss": "^0.0.4",
    "vue-router": "^4.0.3",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/json-schema-merge-allof": "^0.6.1",
    "@types/lodash.topath": "^4.5.8",
    "@types/lodash.union": "^4.6.7",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/babel-plugin-jsx": "^1.1.5",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.0.5",
    "monaco-editor": "^0.41.0",
    "prettier": "^2.4.1",
    "rimraf": "^5.0.5",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5"
  }
}
